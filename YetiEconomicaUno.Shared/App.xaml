    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:YetiEconomicaUno" 
    xmlns:localConverters="using:YetiEconomicaUno.Converters" 
    xmlns:converters="using:CommunityToolkit.WinUI.UI.Converters"
    RequestedTheme="Dark"
    xmlns:themes="using:YetiEconomicaUno.Themes">
	
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />

                <themes:DataTemplatesResourceDictionary Source="Themes/DataTemplates.xaml"/>
                <ResourceDictionary Source="Themes/Generic.xaml"/>

                <!-- Add resource dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Add resources here -->
        </ResourceDictionary>
    </Application.Resources>

</Application>
