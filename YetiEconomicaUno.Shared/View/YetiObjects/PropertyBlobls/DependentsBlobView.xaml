<local:BaseBlobView
    x:Class="YetiEconomicaUno.View.YetiObjects.PropertyBlobls.DependentsBlobView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:YetiEconomicaUno.View.YetiObjects" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <local:BaseBlobView.Detals>
        <StackPanel Orientation="Horizontal" Spacing="5">
            <local:YetiObjectSelector x:Name="RequredBox"
	                                      HorizontalAlignment="Left"
	                                      Mask="RequiredInDependencies"
	                                      Header="Required"/>
            <local:YetiObjectSelector x:Name="VisibleBox"
	                                      HorizontalAlignment="Left"
	                                      Mask="RequiredInDependencies"
	                                      Header="Visibility after"/>
        </StackPanel>
    </local:BaseBlobView.Detals>
</local:BaseBlobView>
